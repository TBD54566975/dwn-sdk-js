{
  "protocol": "http://recipient-can-protocol.xyz",
  "published": true,
  "types": {
    "post": {},
    "tag": {}
  },
  "structure": {
    "post": {
      "$actions": [
        {
          "who": "recipient",
          "can": [
            "co-update",
            "co-delete"
          ]
        }
      ],
      "tag": {
        "$actions": [
          {
            "who": "recipient",
            "of": "post",
            "can": [
              "co-update",
              "co-delete"
            ]
          }
        ]
      }
    }
  }
}