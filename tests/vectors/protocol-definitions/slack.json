{
  "published": true,
  "protocol": "https://slick.app",
  "types": {
    "community": {
      "dataFormats": [
        "application/json"
      ]
    },
    "openChannel": {
      "dataFormats": [
        "application/json"
      ]
    },
    "gatedChannel": {
      "dataFormats": [
        "application/json"
      ]
    },
    "message": {
      "dataFormats": [
        "application/json"
      ]
    },
    "media": {
      "dataFormats": [
        "image/gif",
        "image/png",
        "image/jpeg",
        "video/mp4"
      ]
    },
    "reaction": {
      "dataFormats": [
        "application/json"
      ]
    },
    "admin": {
      "dataFormats": [
        "application/json"
      ]
    },
    "member": {
      "dataFormats": [
        "application/json"
      ]
    },
    "participant": {
      "dataFormats": [
        "application/json"
      ]
    }
  },
  "structure": {
    "community": {
      "admin": {
        "$role": true,
        "$actions": [
          {
            "who": "author",
            "of": "community",
            "can": "write"
          },
          {
            "who": "author",
            "of": "community",
            "can": "delete"
          },
          {
            "role": "community/admin",
            "can": "write"
          },
          {
            "role": "community/admin",
            "can": "delete"
          }
        ]
      },
      "member": {
        "$role": true,
        "$actions": [
          {
            "role": "community/admin",
            "can": "write"
          },
          {
            "role": "community/admin",
            "can": "delete"
          }
        ]
      },
      "openChannel": {
        "$actions": [
          {
            "role": "community/admin",
            "can": "write"
          },
          {
            "role": "community/admin",
            "can": "delete"
          }
        ],
        "message": {
          "$actions": [
            {
              "who": "recipient",
              "of": "community/openChannel/message",
              "can": "write"
            },
            {
              "role": "community/member",
              "can": "write"
            },
            {
              "role": "community/member",
              "can": "delete"
            }
          ],
          "media": {
            "$actions": [
              {
                "who": "author",
                "of": "community/openChannel/message",
                "can": "write"
              }
            ]
          },
          "reaction": {
            "$actions": [
              {
                "role": "community/member",
                "can": "write"
              },
              {
                "role": "community/member",
                "can": "delete"
              }
            ]
          }
        }
      },
      "gatedChannel": {
        "$actions": [
          {
            "role": "community/admin",
            "can": "write"
          },
          {
            "role": "community/admin",
            "can": "delete"
          }
        ],
        "participant": {
          "$role": true,
          "$actions": [
            {
              "who": "author",
              "of": "community/gatedChannel",
              "can": "write"
            },
            {
              "who": "author",
              "of": "community/gatedChannel",
              "can": "delete"
            },
            {
              "role": "community/gatedChannel/participant",
              "can": "write"
            },
            {
              "role": "community/gatedChannel/participant",
              "can": "delete"
            }
          ]
        },
        "message": {
          "$actions": [
            {
              "who": "recipient",
              "of": "community/gatedChannel/message",
              "can": "write"
            },
            {
              "role": "community/gatedChannel/participant",
              "can": "write"
            },
            {
              "role": "community/gatedChannel/participant",
              "can": "query"
            },
            {
              "role": "community/gatedChannel/participant",
              "can": "delete"
            }
          ],
          "media": {
            "$actions": [
              {
                "who": "author",
                "of": "community/gatedChannel/message",
                "can": "write"
              }
            ]
          },
          "reaction": {
            "$actions": [
              {
                "role": "community/gatedChannel/participant",
                "can": "write"
              },
              {
                "role": "community/gatedChannel/participant",
                "can": "delete"
              }
            ]
          }
        }
      }
    }
  }
}