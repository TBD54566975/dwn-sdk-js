{
  "protocol": "http://dex.xyz",
  "published": true,
  "types": {
    "ask": {
      "schema": "https://tbd/website/tbdex/ask",
      "dataFormats": [
        "application/json"
      ]
    },
    "offer": {
      "schema": "https://tbd/website/tbdex/offer",
      "dataFormats": [
        "application/json"
      ]
    },
    "fulfillment": {
      "schema": "https://tbd/website/tbdex/fulfillment",
      "dataFormats": [
        "application/json"
      ]
    }
  },
  "structure": {
    "ask": {
      "$actions": [
        {
          "who": "anyone",
          "can": [
            "create"
          ]
        }
      ],
      "offer": {
        "$actions": [
          {
            "who": "recipient",
            "of": "ask",
            "can": [
              "create"
            ]
          }
        ],
        "fulfillment": {
          "$actions": [
            {
              "who": "recipient",
              "of": "ask/offer",
              "can": [
                "create"
              ]
            }
          ]
        }
      }
    }
  }
}